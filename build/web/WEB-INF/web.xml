<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>IniciarSesion</servlet-name>
        <servlet-class>IniciarSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroAdmin</servlet-name>
        <servlet-class>RegistroAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DonacionPayU</servlet-name>
        <servlet-class>DonacionPayU</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SerAliado</servlet-name>
        <servlet-class>SerAliado</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SerVoluntario</servlet-name>
        <servlet-class>SerVoluntario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Contactenos</servlet-name>
        <servlet-class>Contactenos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IniciarSesion</servlet-name>
        <url-pattern>/IniciarSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroAdmin</servlet-name>
        <url-pattern>/RegistroAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DonacionPayU</servlet-name>
        <url-pattern>/DonacionPayU</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SerAliado</servlet-name>
        <url-pattern>/SerAliado</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SerVoluntario</servlet-name>
        <url-pattern>/SerVoluntario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Contactenos</servlet-name>
        <url-pattern>/Contactenos</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
